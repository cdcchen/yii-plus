{
    "name": "cdcchen/yii-plus",
    "description": "yii plus",
    "version": "1.0.0",
    "keywords": ["yii2", "shortcut", "yii plus", "yii2 extension"],
    "type": "yii2-extension",
    "homepage": "https://github.com/cdcchen/yii-plus",
    "time": "2015-05-25 17:45:01",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/cdcchen/yii-plus/issues",
        "wiki": "https://github.com/cdcchen/yii-plus/wiki",
        "source": "https://github.com/cdcchen/yii-plus"
    },
    "authors": [
        {
            "name": "Dong Chen",
            "email": "cdcchen@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*"
    },
    "require-dev": {
        "cdcchen/php-plus": "1.0.x-dev"
    },
    "autoload": {
        "psr-4": {
            "yiiplus\\": "src/yiiplus/",
            "yiiext\\": "src/yiiext/"
        }
    },
    "extra": {
        "asset-installer-paths": {
          "npm-asset-library": "vendor/npm",
          "bower-asset-library": "vendor/bower"
        },
        "branch-alias": {
          "dev-master": "1.0.x-dev"
        }
    },
    "repositories": [
      {"type": "composer", "url": "http://comproxy.cn/repo/packagist"},
      {"packagist": false}
    ]
}
